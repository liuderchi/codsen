{
  "private": true,
  "name": "codsen-com",
  "version": "2.0.0",
  "description": "codsen.com website",
  "license": "UNLICENSED",
  "scripts": {
    "build": "yarn run build:css:prod && yarn run build:remix && yarn run build:server",
    "build:css": "sass --load-path ../../node_modules styles:app/styles app/components",
    "build:css:prod": "yarn run build:css --env production",
    "build:remix": "cross-env NODE_ENV=production dotenv -e .env remix build --sourcemap",
    "build:server": "node ./other/build-server.js",
    "css:watch": "yarn run build:css --watch",
    "clean": "rimraf ./node_modules/.cache ./server/dist ./build ./public/build \"./app/styles/**/*.*\"",
    "dev": "pm2-dev ./other/pm2.config.js",
    "start": "cross-env NODE_ENV=production node --require dotenv/config ./index.js",
    "postinstall": "remix setup",
    "prebuild": "npm run clean && echo All clean âœ¨",
    "setup": "yarn && docker compose up -d",
    "test": "echo 'would test'"
  },
  "dependencies": {
